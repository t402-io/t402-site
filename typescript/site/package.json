{
  "name": "site",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.7.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:vercel": "cd .. && pnpm install && cd site && pnpm -r --filter=site... run build",
    "start": "next start",
    "format": "prettier -c .prettierrc --write \"**/*.{ts,js,cjs,json,md}\"",
