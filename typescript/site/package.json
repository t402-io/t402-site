    "@solana/kit": "^2.1.1",
    "@vercel/functions": "^2.2.8",
    "@t402/paywall": "workspace:*",
    "motion": "^11.18.0",
    "next": "^16.0.10",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "viem": "^2.21.26",
    "wagmi": "^2.15.6",
    "@t402/core": "workspace:*",
    "x402-legacy": "npm:x402@0.1.2",
